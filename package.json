    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "electron": "^2.0.5",
    "electron-builder": "^20.22.0",
    "react-scripts": "1.1.4",
    "semistandard": "^12.0.1",
    "snazzy": "^7.1.1"
